<h2>Your Appointments</h2>


    <table>
        <% appointments.forEach(appointment => { %> 
            <tr>
           <th><%= appointment.name %></th> 
           <th><%= appointment.doctor %></th> 
           <th><%= appointment.appDate %></th> 
           <th><%= appointment.time %></th> 
           <th>link</th> 
           </tr> 
       <% }) %> 
   
   </table>