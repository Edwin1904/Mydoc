
<div class="book-grid">
  <% doctors.forEach(doctor => { %>
    <div>
      <a href="/<%= doctor.id %>">
        <img height="150" width="100" src="<%= doctor.coverImagePath %>">
      </a>
      <div>Dr. <%= doctor.name %> <%= doctor.surname %> </div>
    <div class="btn-row">
          <a class="btn btn-primary" href="/physician/<%= doctor.id %>">Login</a>
    </div>
  </div>  
  <% }) %>
</div>